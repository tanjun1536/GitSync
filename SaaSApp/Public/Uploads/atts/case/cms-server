/*Table structure for table `appresource` */

DROP TABLE IF EXISTS `appresource`;

CREATE TABLE `appresource` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `type` varchar(255) DEFAULT NULL,
  `version` double DEFAULT NULL,
  `link` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `appresource` */

/*Table structure for table `article` */

DROP TABLE IF EXISTS `article`;

CREATE TABLE `article` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) DEFAULT NULL,
  `subTitle` varchar(255) DEFAULT NULL,
  `summary` varchar(255) DEFAULT NULL,
  `contentLink` varchar(255) DEFAULT NULL,
  `editor` varchar(255) DEFAULT NULL,
  `sender` varchar(255) DEFAULT NULL,
  `isHot` bit(1) DEFAULT NULL,
  `source` varchar(255) DEFAULT NULL,
  `content` text,
  `createDate` datetime DEFAULT NULL,
  `browseCount` int(11) DEFAULT NULL,
  `domain` varchar(255) DEFAULT NULL,
  `focusImage` varchar(255) DEFAULT NULL,
  `titleImage` varchar(255) DEFAULT NULL,
  `keywords` varchar(255) DEFAULT NULL,
  `state` int(11) DEFAULT NULL,
  `sectionId` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK379164D64EF8C4BF` (`sectionId`),
  KEY `FK379164D6549AB6C6` (`state`),
  CONSTRAINT `FK379164D6549AB6C6` FOREIGN KEY (`state`) REFERENCES `state` (`id`),
  CONSTRAINT `FK379164D64EF8C4BF` FOREIGN KEY (`sectionId`) REFERENCES `section` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `article` */

/*Table structure for table `articleaudit` */

DROP TABLE IF EXISTS `articleaudit`;

CREATE TABLE `articleaudit` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userId` int(11) DEFAULT NULL,
  `userName` varchar(255) DEFAULT NULL,
  `reason` varchar(255) DEFAULT NULL,
  `auditDate` datetime DEFAULT NULL,
  `stateId` int(11) DEFAULT NULL,
  `articleId` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FKF0A4DCA54C2D2DB4` (`articleId`),
  KEY `FKF0A4DCA5DCFBE3A1` (`stateId`),
  CONSTRAINT `FKF0A4DCA5DCFBE3A1` FOREIGN KEY (`stateId`) REFERENCES `state` (`id`),
  CONSTRAINT `FKF0A4DCA54C2D2DB4` FOREIGN KEY (`articleId`) REFERENCES `articlemobile` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=88 DEFAULT CHARSET=utf8;

