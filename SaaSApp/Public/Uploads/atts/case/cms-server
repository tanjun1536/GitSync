/*Table structure for table `padsectionfocus` */

DROP TABLE IF EXISTS `padsectionfocus`;

CREATE TABLE `padsectionfocus` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `section` int(11) DEFAULT NULL,
  `imagePath` varchar(255) DEFAULT NULL,
  `focusLink` varchar(255) DEFAULT NULL,
  `title` varchar(255) DEFAULT NULL,
  `content` varchar(255) DEFAULT NULL,
  `article` int(11) DEFAULT NULL,
  `orderNumber` int(11) DEFAULT NULL,
  `state` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK3F7E506ECDCDCE4` (`section`),
  KEY `FK3F7E506AD210CA8` (`article`),
  KEY `FK3F7E506549AB6C6` (`state`),
  CONSTRAINT `FK3F7E506549AB6C6` FOREIGN KEY (`state`) REFERENCES `state` (`id`),
  CONSTRAINT `FK3F7E506AD210CA8` FOREIGN KEY (`article`) REFERENCES `articlepad` (`id`),
  CONSTRAINT `FK3F7E506ECDCDCE4` FOREIGN KEY (`section`) REFERENCES `section` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=237 DEFAULT CHARSET=utf8;

