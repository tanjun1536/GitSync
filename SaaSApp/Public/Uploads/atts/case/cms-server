/*Table structure for table `sectionadapter` */

DROP TABLE IF EXISTS `sectionadapter`;

CREATE TABLE `sectionadapter` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8;

