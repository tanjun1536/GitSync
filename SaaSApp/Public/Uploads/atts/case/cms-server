/*Table structure for table `sectionfocus` */

DROP TABLE IF EXISTS `sectionfocus`;

CREATE TABLE `sectionfocus` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `section` int(11) DEFAULT NULL,
  `imagePath` varchar(255) DEFAULT NULL,
  `focusLink` varchar(255) DEFAULT NULL,
  `title` varchar(255) DEFAULT NULL,
  `content` varchar(255) DEFAULT NULL,
  `article` int(11) DEFAULT NULL,
  `orderNumber` int(11) DEFAULT NULL,
  `state` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK2F2EC113ECDCDCE4` (`section`),
  KEY `FK2F2EC113FF2C8619` (`article`),
  KEY `FK2F2EC113549AB6C6` (`state`),
  CONSTRAINT `FK2F2EC113549AB6C6` FOREIGN KEY (`state`) REFERENCES `state` (`id`),
  CONSTRAINT `FK2F2EC113ECDCDCE4` FOREIGN KEY (`section`) REFERENCES `section` (`id`),
  CONSTRAINT `FK2F2EC113FF2C8619` FOREIGN KEY (`article`) REFERENCES `articlemobile` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=234 DEFAULT CHARSET=utf8;

