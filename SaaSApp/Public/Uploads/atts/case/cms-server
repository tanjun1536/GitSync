/*Table structure for table `padimagecontent` */

DROP TABLE IF EXISTS `padimagecontent`;

CREATE TABLE `padimagecontent` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) DEFAULT NULL,
  `subTitle` varchar(255) DEFAULT NULL,
  `summary` varchar(255) DEFAULT NULL,
  `editor` varchar(255) DEFAULT NULL,
  `sender` varchar(255) DEFAULT NULL,
  `source` varchar(255) DEFAULT NULL,
  `createDate` datetime DEFAULT NULL,
  `browseCount` int(11) DEFAULT NULL,
  `keywords` varchar(255) DEFAULT NULL,
  `state` int(11) DEFAULT NULL,
  `sectionId` int(11) DEFAULT NULL,
  `template` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK3AF46EB14EF8C4BF` (`sectionId`),
  KEY `FK3AF46EB1549AB6C6` (`state`),
  KEY `FK3AF46EB1B1E6B48B` (`template`),
  CONSTRAINT `FK3AF46EB1B1E6B48B` FOREIGN KEY (`template`) REFERENCES `template` (`id`),
  CONSTRAINT `FK3AF46EB14EF8C4BF` FOREIGN KEY (`sectionId`) REFERENCES `section` (`id`),
  CONSTRAINT `FK3AF46EB1549AB6C6` FOREIGN KEY (`state`) REFERENCES `state` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `padimagecontent` */

/*Table structure for table `padimagecontentaudit` */

DROP TABLE IF EXISTS `padimagecontentaudit`;

CREATE TABLE `padimagecontentaudit` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userId` int(11) DEFAULT NULL,
  `userName` varchar(255) DEFAULT NULL,
  `reason` varchar(255) DEFAULT NULL,
  `auditDate` datetime DEFAULT NULL,
  `stateId` int(11) DEFAULT NULL,
  `imgContentId` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK6474C3AA820C620F` (`imgContentId`),
  KEY `FK6474C3AADCFBE3A1` (`stateId`),
  CONSTRAINT `FK6474C3AADCFBE3A1` FOREIGN KEY (`stateId`) REFERENCES `state` (`id`),
  CONSTRAINT `FK6474C3AA820C620F` FOREIGN KEY (`imgContentId`) REFERENCES `padimagecontent` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `padimagecontentaudit` */

/*Table structure for table `padpagelayout` */

DROP TABLE IF EXISTS `padpagelayout`;

