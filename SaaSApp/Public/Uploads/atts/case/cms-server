/*Table structure for table `userresource` */

DROP TABLE IF EXISTS `userresource`;

CREATE TABLE `userresource` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `resourceId` int(11) DEFAULT NULL,
  `resourceName` varchar(255) DEFAULT NULL,
  `resourceType` varchar(255) DEFAULT NULL,
  `resourceVersion` varchar(255) DEFAULT NULL,
  `uniqueCode` varchar(255) DEFAULT NULL,
  `userId` int(11) DEFAULT NULL,
  `userName` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `userresource` */

/*Table structure for table `version` */

DROP TABLE IF EXISTS `version`;

CREATE TABLE `version` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `version` varchar(255) DEFAULT NULL,
  `osVersion` varchar(255) DEFAULT NULL,
  `equType` varchar(255) DEFAULT NULL,
  `equHeight` varchar(255) DEFAULT NULL,
  `equWidth` varchar(255) DEFAULT NULL,
  `downUrl` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

