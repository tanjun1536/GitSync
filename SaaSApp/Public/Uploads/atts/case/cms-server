/*Table structure for table `state` */

DROP TABLE IF EXISTS `state`;

CREATE TABLE `state` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `nextId` int(11) DEFAULT NULL,
  `prevId` int(11) DEFAULT NULL,
  `role` int(11) DEFAULT NULL,
  `rejected` int(11) DEFAULT NULL,
  `nodeType` int(11) DEFAULT NULL,
  `nodeDesc` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK4C7D471F8C9983` (`nextId`),
  KEY `FK4C7D47113A4E0C3` (`prevId`),
  KEY `FK4C7D4718B216307` (`role`),
  KEY `FK4C7D47129CB06B3` (`rejected`),
  CONSTRAINT `FK4C7D47129CB06B3` FOREIGN KEY (`rejected`) REFERENCES `state` (`id`),
  CONSTRAINT `FK4C7D47113A4E0C3` FOREIGN KEY (`prevId`) REFERENCES `state` (`id`),
  CONSTRAINT `FK4C7D4718B216307` FOREIGN KEY (`role`) REFERENCES `role` (`id`),
  CONSTRAINT `FK4C7D471F8C9983` FOREIGN KEY (`nextId`) REFERENCES `state` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=47 DEFAULT CHARSET=utf8;

