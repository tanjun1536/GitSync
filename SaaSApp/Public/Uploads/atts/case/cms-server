  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) DEFAULT NULL,
  `createDate` datetime DEFAULT NULL,
  `updateDate` datetime DEFAULT NULL,
  `domain` varchar(255) DEFAULT NULL,
  `link` varchar(255) DEFAULT NULL,
  `state` int(11) DEFAULT NULL,
  `section` int(11) DEFAULT NULL,
  `createUser` int(11) DEFAULT NULL,
  `updateUser` int(11) DEFAULT NULL,
  `template` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK5327436CB612A8FF` (`createUser`),
  KEY `FK5327436C52D2924C` (`updateUser`),
  KEY `FK5327436CB1E6B48B` (`template`),
  CONSTRAINT `FK5327436CB1E6B48B` FOREIGN KEY (`template`) REFERENCES `template` (`id`),
  CONSTRAINT `FK5327436C52D2924C` FOREIGN KEY (`updateUser`) REFERENCES `backuser` (`id`),
