/*Table structure for table `backuser_role` */

DROP TABLE IF EXISTS `backuser_role`;

CREATE TABLE `backuser_role` (
  `UserId` int(11) NOT NULL,
  `RoleId` int(11) NOT NULL,
  PRIMARY KEY (`UserId`,`RoleId`),
  KEY `FKA2DA474353C32AA2` (`RoleId`),
  KEY `FKA2DA474332922C5E` (`UserId`),
  CONSTRAINT `FKA2DA474332922C5E` FOREIGN KEY (`UserId`) REFERENCES `backuser` (`id`),
  CONSTRAINT `FKA2DA474353C32AA2` FOREIGN KEY (`RoleId`) REFERENCES `role` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

