/*Table structure for table `mobileimagecontentaudit` */

DROP TABLE IF EXISTS `mobileimagecontentaudit`;

CREATE TABLE `mobileimagecontentaudit` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userId` int(11) DEFAULT NULL,
  `userName` varchar(255) DEFAULT NULL,
  `reason` varchar(255) DEFAULT NULL,
  `auditDate` datetime DEFAULT NULL,
  `stateId` int(11) DEFAULT NULL,
  `imgContentId` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK159BC73B8795EC62` (`imgContentId`),
  KEY `FK159BC73BDCFBE3A1` (`stateId`),
  CONSTRAINT `FK159BC73BDCFBE3A1` FOREIGN KEY (`stateId`) REFERENCES `state` (`id`),
  CONSTRAINT `FK159BC73B8795EC62` FOREIGN KEY (`imgContentId`) REFERENCES `imagecontent` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `mobileimagecontentaudit` */

/*Table structure for table `padarticleaudit` */

DROP TABLE IF EXISTS `padarticleaudit`;

CREATE TABLE `padarticleaudit` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userId` int(11) DEFAULT NULL,
  `userName` varchar(255) DEFAULT NULL,
  `reason` varchar(255) DEFAULT NULL,
  `auditDate` datetime DEFAULT NULL,
  `stateId` int(11) DEFAULT NULL,
  `articleId` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FKC56E0098FA21B443` (`articleId`),
  KEY `FKC56E0098DCFBE3A1` (`stateId`),
  CONSTRAINT `FKC56E0098DCFBE3A1` FOREIGN KEY (`stateId`) REFERENCES `state` (`id`),
  CONSTRAINT `FKC56E0098FA21B443` FOREIGN KEY (`articleId`) REFERENCES `articlepad` (`id`)
