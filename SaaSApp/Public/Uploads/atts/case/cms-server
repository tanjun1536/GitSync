/*Table structure for table `imagecontent` */

DROP TABLE IF EXISTS `imagecontent`;

CREATE TABLE `imagecontent` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) DEFAULT NULL,
  `subTitle` varchar(255) DEFAULT NULL,
  `summary` varchar(255) DEFAULT NULL,
  `editor` varchar(255) DEFAULT NULL,
  `sender` varchar(255) DEFAULT NULL,
  `source` varchar(255) DEFAULT NULL,
  `domain` varchar(255) DEFAULT NULL,
  `contentLink` varchar(255) DEFAULT NULL,
  `isHot` bit(1) DEFAULT NULL,
  `content` varchar(255) DEFAULT NULL,
  `focusImage` varchar(255) DEFAULT NULL,
  `titleImage` varchar(255) DEFAULT NULL,
  `createDate` datetime DEFAULT NULL,
  `browseCount` int(11) DEFAULT NULL,
  `keywords` varchar(255) DEFAULT NULL,
  `state` int(11) DEFAULT NULL,
  `sectionId` int(11) DEFAULT NULL,
  `template` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK662B4ABE4EF8C4BF` (`sectionId`),
  KEY `FK662B4ABE549AB6C6` (`state`),
  KEY `FK662B4ABEB1E6B48B` (`template`),
  CONSTRAINT `FK662B4ABEB1E6B48B` FOREIGN KEY (`template`) REFERENCES `template` (`id`),
  CONSTRAINT `FK662B4ABE4EF8C4BF` FOREIGN KEY (`sectionId`) REFERENCES `section` (`id`),
  CONSTRAINT `FK662B4ABE549AB6C6` FOREIGN KEY (`state`) REFERENCES `state` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `imagecontent` */

/*Table structure for table `imageinfo` */

DROP TABLE IF EXISTS `imageinfo`;

CREATE TABLE `imageinfo` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `path` varchar(255) DEFAULT NULL,
  `des` varchar(255) DEFAULT NULL,
  `sort` int(11) DEFAULT NULL,
  `userId` int(11) DEFAULT NULL,
  `sectionId` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK43157C894EF8C4BF` (`sectionId`),
  KEY `FK43157C8932922C5E` (`userId`),
  CONSTRAINT `FK43157C8932922C5E` FOREIGN KEY (`userId`) REFERENCES `backuser` (`id`),
  CONSTRAINT `FK43157C894EF8C4BF` FOREIGN KEY (`sectionId`) REFERENCES `section` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=225 DEFAULT CHARSET=utf8;

