/*Table structure for table `articlemobilecomment` */

DROP TABLE IF EXISTS `articlemobilecomment`;

CREATE TABLE `articlemobilecomment` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `article` int(11) DEFAULT NULL,
  `userId` int(11) DEFAULT NULL,
  `title` varchar(50) DEFAULT NULL,
  `content` varchar(255) DEFAULT NULL,
  `postDate` datetime DEFAULT NULL,
  `auditStatus` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK37E73227FF2C8619` (`article`),
  KEY `FK37E73227AF39EF94` (`userId`),
  CONSTRAINT `FK37E73227AF39EF94` FOREIGN KEY (`userId`) REFERENCES `frontuser` (`id`),
  CONSTRAINT `FK37E73227FF2C8619` FOREIGN KEY (`article`) REFERENCES `articlemobile` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `articlemobilecomment` */

/*Table structure for table `articlepad` */

DROP TABLE IF EXISTS `articlepad`;

CREATE TABLE `articlepad` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(255) DEFAULT NULL,
  `subTitle` varchar(255) DEFAULT NULL,
  `summary` varchar(2000) DEFAULT NULL,
  `contentLink` varchar(255) DEFAULT NULL,
  `editor` varchar(255) DEFAULT NULL,
  `sender` varchar(255) DEFAULT NULL,
  `isHot` bit(1) DEFAULT NULL,
  `source` varchar(255) DEFAULT NULL,
  `content` text,
  `createDate` datetime DEFAULT NULL,
  `browseCount` int(11) DEFAULT NULL,
  `domain` varchar(255) DEFAULT NULL,
  `focusImage` varchar(255) DEFAULT NULL,
  `titleImage` varchar(255) DEFAULT NULL,
  `keywords` varchar(255) DEFAULT NULL,
  `state` int(11) DEFAULT NULL,
  `template` int(11) DEFAULT NULL,
  `sectionId` int(11) DEFAULT NULL,
  `topDate` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK80A69BDD4EF8C4BF` (`sectionId`),
  KEY `FK80A69BDD549AB6C6` (`state`),
  KEY `FK80A69BDDB1E6B48B` (`template`),
  CONSTRAINT `FK80A69BDD4EF8C4BF` FOREIGN KEY (`sectionId`) REFERENCES `section` (`id`),
  CONSTRAINT `FK80A69BDD549AB6C6` FOREIGN KEY (`state`) REFERENCES `state` (`id`),
  CONSTRAINT `FK80A69BDDB1E6B48B` FOREIGN KEY (`template`) REFERENCES `template` (`id`)
