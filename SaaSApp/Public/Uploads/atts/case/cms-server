  `padTitleImage` varchar(255) DEFAULT NULL,
  `articleCount` int(11) DEFAULT NULL,
  `createDate` datetime DEFAULT NULL,
  `adapterTerminal` int(11) DEFAULT NULL,
  `parentId` int(11) DEFAULT NULL,
  `sectionType` int(11) DEFAULT NULL,
  `showType` int(11) DEFAULT NULL,
  `commentAudit` bit(1) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FKD8A816C5BD78F144` (`parentId`),
  KEY `FKD8A816C547FA5D78` (`sectionType`),
  KEY `FKD8A816C5C600671B` (`adapterTerminal`),
  KEY `FKD8A816C522700EAD` (`showType`),
  CONSTRAINT `FKD8A816C522700EAD` FOREIGN KEY (`showType`) REFERENCES `sectionshowtype` (`id`),
  CONSTRAINT `FKD8A816C547FA5D78` FOREIGN KEY (`sectionType`) REFERENCES `sectiontype` (`id`),
  CONSTRAINT `FKD8A816C5BD78F144` FOREIGN KEY (`parentId`) REFERENCES `section` (`id`),
  CONSTRAINT `FKD8A816C5C600671B` FOREIGN KEY (`adapterTerminal`) REFERENCES `sectionadapter` (`id`)
